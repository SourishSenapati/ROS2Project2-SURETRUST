# GOAL: Restoration Protocol Parameters
string protocol_id        # e.g. "TEST-RUN-01"
float32 target_temp_k     # Desired Temperature (Kelvin)
float32 limit_pressure_pa # Safety Pressure Limit (Pascals)
float32 duration_sec      # How long to run the batch

---
# RESULT: Final Process Verdict
bool safe_shutdown        # True = Success, False = Scram
string verdict_code       # "OPTIMAL", "OVER_PRESSURE", "MANUAL_ABORT"
float32 final_yield       # Calculated product quality %

---
# FEEDBACK: Real-time Telemetry
float32 current_temp_k
float32 current_pressure_pa
float32 heat_gen_watts    # Energy generated by reaction
float32 progress_percent
string system_status      # "HEATING", "STABLE", "CRITICAL"
